{
  "manifest_version": 3,
  "name": "Weather",
  "version": "1.0.0",
  "description": "Monitor weather wherever you go",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "src/assets/icon.png",
    "48": "src/assets/icon.png",
    "128": "src/assets/icon.png"
  }
}
